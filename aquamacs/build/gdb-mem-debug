# Some memory debugging configuration for gdb with Aquamacs

# Run gdb in the source directory, and it should load the .gdbinit
# there with the Emacs gdb macros.

set environment NSDebugEnabled="YES"
set environment NSZombieEnabled="YES"
#export NSHangOnUncaughtException="YES"
set environment NSAutoreleaseFreedObjectCheckEnabled="YES"
